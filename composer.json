{
  "name": "viswoole/viswoole",
  "description": "ViSwoole, 这是一个基于swoole扩展的高性能PHP框架。",
  "keywords": [
    "framework",
    "viswoole"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "zhuchonglin",
      "email": "8210856@qq.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": ">=5.1",
    "viswoole/core": "@dev"
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "php viswoole service:discover",
      "php viswoole command:discover"
    ]
  }
}
